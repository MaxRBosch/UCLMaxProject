e } from '@ionic/storage';\nimport { findLast } from '@angular/compiler/src/directive_resolver';\nimport { Predictions } from '../btc/btc.page';\nimport ChartDataLabels from 'chartjs-plugin-datalabels';\n\nimport Swal from 'sweetalert2'\nimport { element } from 'protractor';\n\n\n@Component({\n  selector: 'app-homepage',\n  templateUrl: './homepage.page.html',\n  styleUrls: ['./homepage.page.scss'],\n})\nexport class HomepagePage implements OnInit {\n  loaded = 'init'\n  time :any\n  public title: string = \"APP_NAME\";\n\n  \n  public predictions: Predictions[] = [];\n  @ViewChild(\"lineCanvas\") lineCanvas: any;\n  private lineChart: Chart;\nprice:number;\n\nbitcoin:any\nethereum:any\ndollars:any\nglob_price_btc:number;\nglob_price_eth:number;\n\n\n\n\n  constructor(private storage: Storage\n    ) { }\n    load(): Promise<boolean> {\n      return new Promise((resolve) => {\n        this.storage.get('predictions').then((predictions) => {\n          if(predictions != null){\n            this.predictions = predictions;             \n          }           \n        });\n        \n          resolve(true);\n          return})\n      ;   \n    }\n    load_porto(): Promise<boolean> {\n      return new Promise((resolve) => {\n        this.storage.get('ethereum').then((ethereum) => {\n  \n          if(ethereum != null){\n            this.ethereum = ethereum;\n           \n            \n          }else{this.ethereum = 0}});\n          this.storage.get('dollars').then((dollars) => {\n    \n            if(dollars != null){\n              this.dollars = dollars;\n            }else{this.dollars = 0}});\n\n        this.storage.get('bitcoin').then((bitcoin) => {\n  \n          if(bitcoin != null){\n            this.bitcoin = bitcoin;\n           \n            \n            \n            \n          }else{this.bitcoin = 0}\n          resolve(true);\n          return})\n      });  \n    }\n  ngOnInit() {  \n    \n\n    \n\n      this.getprice();\n       this.load();\n    \n    \n    \n\n\n  }\n  \n  async ionViewDidEnter () {\n    await this.load();\n    await this.load_porto();\n    await this.get_chart()\n  }\n  \n\n  async getprice()\n  {\n    \n    var price_eth :number\n    var price_btc :number\n    try{\n    var url_btc = \"https://api.binance.com/api/v1/klines?symbol=BTCUSDT&interval=1m\";\n    var url_eth = \"https://api.binance.com/api/v1/klines?symbol=ETHUSDT&interval=1m\"\n    const btc = await axios.get(url_btc)\n    const eth = await axios.get(url_eth)\n\n    if (btc.data.length != 0 && eth.data.length != 0){\n      this.loaded = 'yes'}\n      else {this.loaded='no'}\n    this.time = new Date(btc.data[btc.data.length-1][0]).toTimeString().replace(/.*(\\d{2}:\\d{2}:\\d{2}).*/, \"$1\");\n    price_btc = Math.floor(btc.data[btc.data.length-1][1])\n    price_eth = Math.floor(eth.data[eth.data.length-1][1])\n\n    //These global variables are declared so they display the price but they are not used for any other functions.\n    this.glob_price_btc = price_btc\n    this.glob_price_eth = price_eth\n\n   \n\n    return {'price_btc':price_btc,'price_eth':price_eth}\n\n    }\n    catch {this.loaded = 'no';\n    return}\n    \n  \n  }\n  async get_chart(){\n    setInterval(async() =>{  this.getprice()},60000);    \n    let values = await this.get_hist_price(this.predictions)\n    Chart.helpers.merge(Chart.defaults.global.plugins.datalabels, {\n      color: '#FE777B'\n  });\n    this.lineChart = new Chart(this.lineCanvas.nativeElement, {\n      type: \"line\",\n      data: {\n        labels: values.time,\n       \n        datasets: [\n          {\n            label: \"Prediction\",\n            fill: true,\n            lineTension: 0.3,\n            backgroundColor: \"rgb(5, 156, 45, 0.6)\",\n            borderColor: \"rgba(220, 220, 220, 1)\",\n            borderCapStyle: \"butt\",\n            borderDash: [],\n            borderDashOffset: 0.0,\n            borderJoinStyle: \"miter\",\n            pointBorderColor: \"rgba(220, 220, 220, 0.5)\",\n            pointBackgroundColor: \"rgba(220,220,220,1)\",\n            pointBorderWidth: 4,\n            pointHoverRadius: 5,\n            pointHoverBackgroundColor: \"rgba(220,220,220,1)\",\n            pointHoverBorderColor: \"rgba(220,220,220,1)\",\n            pointHoverBorderWidth: 2,\n            pointRadius: 4,\n            pointHitRadius: 10,\n            data: values.pred_price,\n            spanGaps: false,\n            datalabels:{\n            display:false},\n            \n          },{\n            label: \"Actual price ($)\",\n            data: values.hist_price,\n            borderWidth: 2,\n            backgroundColor: \"red\",\n            borderColor: \"red\",\n            pointBackgroundColor: \"red\",\n            pointBorderColor: \"red\",\n            pointHoverBackgroundColor: \"red\",\n            pointHoverBorderColor: \"red\",\n            type: 'scatter',\n            datalabels: {\n              color: 'black',\n              align: 'end',\n              display: false\n              \n              \t\n          }\n            \n         }]\n      },plugins: [ChartDataLabels],options: {  \n    maintainAspectRatio: false, \n        responsive: true, \n        title: {\n          display: false,\n          text: 'Bitcoin price ($) in 2020'\n      },    \n        legend: {\n          display: true,\n          position:'bottom',\n          labels:\n          {boxWidth:10,\n            usePointStyle:true}\n        },scales: {\n          xAxes: [{\n              gridLines: {\ndisplay:false,\n                  borderDash: [8, 4],\n                  color: \"#348632\"\n              },\n              \n          }],\n  \n          yAxes: [{\n              gridLines: {\n                display:false,\n                 \n              }, ticks: {fontSize: 10,\n                maxTicksLimit: 4.1,\n                    }\n          }]\n      }\n      }\n      \n    });\n    return\n  }\n\n  async get_hist_price(predictions){\n\n   var hist_time:any =[]\n   var hist_price:any =[]\n   var pred_price:any =[]\n   var url = \"https://api.binance.com/api/v1/klines?symbol=BTCUSDT&interval=1h\";\n  const res = await axios.get(url)\n  \n  var j;\n  for (j = 0; j < predictions.length; j++) {\n    var date = new Date(predictions[j].timestamp);\n    var month = date.getMonth() +1; \n    var day =date.getDate(); \n    var hour = date.getHours(); \n    hist_time.push( day +\"/\"+month+\"-\"+hour )\n    pred_price.push(predictions[j].price)\n    var i;\n    for (i = 0; i < res.data.length; i++) {\n      var date_api = new Date(res.data[i][0])\n      if (date_api.getMonth()+1 == month && date_api.getDate() == day && date_api.getHours() == hour){\n        hist_price.push(Math.floor(res.data[i][1]))}}\n    \n      \n    }\n    console.log(hist_time.length)\n  \n  \n  return {'time':hist_time,'hist_price':hist_price,'pred_price':pred_price}}\n\n\nasync buy(){\n  let prices = await this.getprice()\n\n  Swal.fire({\n    title: 'Do you want to buy Bitcoin or Ethereum?',\n    icon: 'info',\n    showCloseButton: true,\n    showCancelButton: true,\n    focusConfirm: false,\n    confirmButtonText:\n      'Bitcoin',\n    cancelButtonText:\n      'Ethereum',\n  }).then(async (result) => {\n    if (result.value) { \n      const{value: buying} = await Swal.fire({\n        title: 'One Bitcoin is currently $'+ prices.price_btc+ ', how many would you like to buy?',\n        input: 'text',\n        showCancelButton: true,\n        inputValidator: (value) => {\n          if (!value) {\n            return 'You need to write something!'\n          }\n        }\n      })      \n      if (buying) {\n        Swal.fire({\n          title: 'Are you sure you want to buy '+buying+\" Bitcoin for $\"+ prices.price_btc*Number(buying)+'?',\n         \n    showCloseButton: true,\n    showCancelButton: true,\n    focusConfirm: false,\n    confirmButtonText:\n      'Yes',\n    cancelButtonText:\n      'No'\n          \n        }).then(async (result) =>{this.bitcoin = this.bitcoin+Number(buying);\n          this.dollars = this.dollars -  prices.price_btc*Number(buying);\n\n          await this.storage.set('bitcoin', this.bitcoin);\n          await this.storage.set('dollars', this.dollars);\n          await this.load_porto();\n\n          return {'dollars':this.dollars,'bitcoin':this.bitcoin} \n        })\n      }\n    } \n    else if (result.dismiss == Swal.DismissReason.cancel){ const{value: buying} = await Swal.fire({\n      title: 'One Ethereum is currently $'+ prices.price_eth+ ', how many would you like to buy?',\n      input: 'text',\n      showCancelButton: true,\n      inputValidator: (value) => {\n        if (!value) {\n          return 'You need to write something!'\n        }\n      }\n    }) \n    if (buying) {\n      Swal.fire({\n        title: 'Are you sure you want to buy '+buying+\" Ethereum for $\"+ prices.price_eth*Number(buying)+'?',\n       \n  showCloseButton: true,\n  showCancelButton: true,\n  focusConfirm: false,\n  confirmButtonText:\n    'Yes',\n  cancelButtonText:\n    'No'\n        \n      }).then(async (result) =>{this.ethereum = this.ethereum+Number(buying);\n        this.dollars = this.dollars -  prices.price_eth*Number(buying);\n\n        await this.storage.set('ethereum', this.ethereum);\n        await this.storage.set('dollars', this.dollars);\n        await this.load_porto(); \n        return {'dollars':this.dollars,'ethereum':this.ethereum} \n\n      })\n      return}\n    return}\n  })}\n  \nasync sell(){\n  let prices = await this.getprice();\n  Swal.fire({\n    title: 'Do you want to sell Bitcoin or Ethereum?',\n    icon: 'info',\n   \n    showCloseButton: true,\n    showCancelButton: true,\n    focusConfirm: false,\n    confirmButtonText:\n      'Bitcoin',\n    cancelButtonText:\n      'Ethereum',\n  }).then(async (result) => {\n    if (result.value) { \n      const{value: buying} = await Swal.fire({\n        title: 'One Bitcoin is currently $'+ prices.price_btc+ '. You have '+this.bitcoin+' bitcoin, how many would you like to sell?',\n        input: 'text',\n        showCancelButton: true,\n        inputValidator: (value) => {\n          if (!value) {\n            return 'You need to write something!'\n          }\n          if (Number(value) > this.bitcoin){          \n            return 'You do not have that many bitcoin!'}\n        }\n      })      \n      if (buying) {\n        Swal.fire({\n          title: 'Are you sure you want to sell '+buying+\" Bitcoin for $\"+ prices.price_btc*Number(buying)+'?',\n         \n    showCloseButton: true,\n    showCancelButton: true,\n    focusConfirm: false,\n    confirmButtonText:\n      'Yes',\n    cancelButtonText:\n      'No'\n          \n        }).then(async (result) =>{this.bitcoin = this.bitcoin-Number(buying);\n          this.dollars = this.dollars +  prices.price_btc*Number(buying);\n\n          await this.storage.set('bitcoin', this.bitcoin);\n          await this.storage.set('dollars', this.dollars);\n          await this.load_porto(); \n          return {'dollars':this.dollars,'bitcoin':this.bitcoin} \n\n        })\n      }\n    } \n    else{ const{value: buying} = await Swal.fire({\n      title: 'One Ethereum is currently $'+ prices.price_eth+ '. You have '+this.ethereum+ ' ethereum, how many would you like to sell?',\n      input: 'text',\n      showCancelButton: true,\n      inputValidator: (value) => {\n        if (!value) {\n          return 'You need to write something!'\n        }\n        if (Number(value) > this.ethereum){          \n          return 'You do not have that many ethereum!'}\n\n      }\n    }) \n    if (buying) {\n      Swal.fire({\n        title: 'Are you sure you want to sell '+buying+\" Ethereum for $\"+ prices.price_eth*Number(buying)+'?',\n       \n  showCloseButton: true,\n  showCancelButton: true,\n  focusConfirm: false,\n  confirmButtonText:\n    'Yes',\n  cancelButtonText:\n    'No'\n        \n      }).then(async (result) =>{this.ethereum = this.ethereum-Number(buying);\n        this.dollars = this.dollars +  prices.price_eth*Number(buying);\n\n        await this.storage.set('ethereum', this.ethereum);\n        await this.storage.set('dollars', this.dollars);\n        await this.load_porto(); \n        return {'dollars':this.dollars,'ethereum':this.ethereum} \n\n      })\n    return}return\n    }})}\n    \n  \n  }"]}